{{#if isAnyNonNull}}
  (ANY_NON_NULL)
{{else}} 
  {{#if isValue}}
    {{rangeComparison}}{{#if value.inclusive?}}={{/if}}
    {{value.value}}{{#if value.unit }} {{translate_unit value.unit value.value}} {{/if}}
  {{else}}
    {{#if isRange}}
      {{#ifCond value.high '&&' value.low}}
        (BOTH)
  {{!-- 
  {{#if (value.high.value == value.low.value and value.high.inclusive and value.low.inclusive) }}
   <%== render partial: 'value', locals: {value: value.low} %>
  else
   %>
  &gt;<%== render partial: 'value', locals: {value: value.low} %> and &lt;<%== render partial: 'value', locals: {value: value.high} %>
   <%
  end 
   --}}
      {{else}}
        {{#if value.high}}
          {{view "ValueLogic" value=value.high rangeComparison="<" tag="span"}}
        {{else}}
          {{#if value.low}}
            {{view "ValueLogic" value=value.low rangeComparison=">" tag="span"}}
          {{/if}}
        {{/if}}
      {{/ifCond}}
    {{else}}
      {{#ifCond value.type '==' 'CD'}}
        : {{value.title}}
      {{/ifCond}}
    {{/if}}
  {{/if}}
{{/if}}

{{!-- <% if value.is_a? HQMF::AnyValue %>
<% elsif value.is_a? HQMF::Value %>

<% elsif value.is_a? HQMF::Range %>
  <% if (value.high && value.low)
       if (value.high.value == value.low.value and value.high.inclusive and value.low.inclusive)
        %>
<%== render partial: 'value', locals: {value: value.low} %>
        <%
       else
         %>
&gt;<%== render partial: 'value', locals: {value: value.low} %> and &lt;<%== render partial: 'value', locals: {value: value.high} %>
         <%
       end 
     elsif value.high
       %>
&lt;<%== render partial: 'value', locals: {value: value.high} %>
       <%
     elsif value.low
       %>
&gt;<%== render partial: 'value', locals: {value: value.low} %>
       <%
     end 
  %>
    
<% elsif value.is_a? HQMF::Coded %>
: <%= value.title %>
<% end %>
 --}}