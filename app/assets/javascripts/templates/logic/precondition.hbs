<ul id="precondition_{{precondition.id}}">
  <li>
    <span class="precondition_{{parentPrecondition.id}} rationale_target">
      {{translate_conjunction parentPrecondition.conjunction_code}}
      {{#if parentPrecondition.negation}} NOT{{/if}}: 
    </span>
    {{#if precondition.preconditions }}
        {{#each precondition.preconditions}}
          {{view "PreconditionLogic" precondition=this parentPrecondition=../precondition dataCriteriaMap=../dataCriteriaMap sourceDataCriteria=../sourceDataCriteria}}
        {{/each}}
    {{else}}
      {{view "DataCriteriaLogic" reference=precondition.reference dataCriteriaMap=../dataCriteriaMap sourceDataCriteria=../sourceDataCriteria tag="span"}}
    {{/if}}
  </li>
</ul>
