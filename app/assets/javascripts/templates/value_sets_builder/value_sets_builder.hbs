  {{else}}
    <div class="panel panel-default">
      <div class="panel-heading">
        <h2 class="panel-title">Black List</h2>
      </div>
      <div class="panel-body">
        {{view blackListCollectionView}}
      </div>
    </div>
  {{/empty}}
</div>