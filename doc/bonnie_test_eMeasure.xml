            <!--
                **************************************************************   
                Population Criteria Section
                **************************************************************
            --><component typeCode="COMP">
      <section>
         <code code="57026-7" codeSystem="2.16.840.1.113883.6.1"
               displayName="Population criteria"/>
         <title>Population criteria</title>
         <text>
            <list>
               <item>
                  <content styleCode="Bold">Initial Patient Population =</content>
                  <list>
                     <item>AND: "Patient Characteristic Birthdate: birth date"  &gt;= 18 year(s) starts before start of "Measurement Period"</item>
                     <item>AND: "Occurrence A of Encounter, Performed: Non-Elective Inpatient Encounter" during "Measurement Period"</item>
                  </list>
               </item>
               <item>
                  <content styleCode="Bold">Denominator =</content>
                  <list>
                     <item>AND: "Initial Patient Population"</item>
                  </list>
               </item>
               <item>
                  <content styleCode="Bold">Denominator Exclusions =</content>
                  <list>
                     <item>AND: "Intervention, Performed: Comfort Measures" starts during "Occurrence A of Encounter, Performed: Non-Elective Inpatient Encounter"</item>
                  </list>
               </item>
               <item>
                  <content styleCode="Bold">Numerator =</content>
                  <list>
                     <item>AND: "Medication, Administered: Hospital Measures-Aspirin" starts during "Occurrence A of Encounter, Performed: Non-Elective Inpatient Encounter"</item>
                  </list>
               </item>
               <item>
                  <content styleCode="Bold">Denominator Exceptions =</content>
                  <list>
                     <item>None</item>
                  </list>
               </item>
            </list>
         </text>
